import java.util.*;
class Main {
    public static void prime(int num){
        for(int i=2;i<=num;i++){
            while(num%i==0){
                System.out.print(i+" ");
                num/=i;
            }
            
        }
    }
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter the number: ");
        int a=sc.nextInt();
        prime(a);
    }
}
