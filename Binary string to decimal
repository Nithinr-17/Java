import java.util.*;
import java.lang.Math;
public class Main
{
	public static void main(String[] args) {
	    String str="1111";
	    int ans=0;
	    int pow=0;
	    for(int i=str.length()-1;i>=0;i--){
	        char ch=str.charAt(i);
	        if(ch=='1'){
	            ans+=Math.pow(2,pow) ;
	        }
	        pow++;
	    }
	    System.out.println(ans);
	   
	   
	}
}
